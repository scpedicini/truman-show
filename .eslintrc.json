{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 13
    },
    "rules": {
        "indent": [
            "error",
            4
        ],
        "linebreak-style": [
            "error"
        ],
        "quotes": [
            "error",
            "single"
        ],
        "no-unused-vars": "off"
    },
    "globals": {
        "Misc": true,
        "ClipboardItem": true,
        "GLightbox": true,
        "bootstrap": true,
        "GoogleClasses": true
    }
}
